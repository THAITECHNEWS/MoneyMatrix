{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "baseUrl": "..",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "ts-node": {
    "esm": false,
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2017"
    }
  }
}



